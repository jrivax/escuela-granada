{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "baseUrl": "./",
    "rootDirs": [
      "src"
    ],
    "paths": {
      "common/*":  ["src/common/*"],
      "layouts/*":  ["src/layouts/*"],
      "svg/*":  ["src/svg/*"],
      "views/*":  ["src/views/*"],
      "theme/*":  ["src/theme/*"]
    }
  },
  "exclude": ["dist", "node_modules", "tmp"],
}
